#include <stdio.h>
#include <string.h>

int main() {
    char name[100], new[100];

    printf("Enter name : ");
    gets(name);

    strcpy(new, name);
    strrev(new);

    if (strcmp(name, new) == 0) {
        printf("Palindrome");
    } else {
        printf("Not Palindrome");
    }

    return 0;
}
