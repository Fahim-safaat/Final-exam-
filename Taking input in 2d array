#include<stdio.h>
int main(){
    int n1,n2;
 printf("Enter the size of A the matrix : ");
 scanf("%d %d", &n1,&n2);

 int a[100][100],i,j;

 //input

 for(i=0;i<n1;i++){
    for(j=0;j<n2;j++){
        printf("Enter the element of the A matrix A[%d][%d]: ", i,j);
        scanf("%d", &a[i][j]);
    }
}
//output of a
 printf("A Matrix is: \n");
 for(i=0;i<n1;i++){
    for(j=0;j<n2;j++){
        printf("%d  ", a[i][j]);
    }
    printf("\n");
}

return 0;
}
